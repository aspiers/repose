{
  "configuration": {
    "monitorInterval": "5",
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d %-4r [%t] %-5p %c - %m%n"
        }
      },
      "List": {
        "name": "List2"
      }
    },
    "loggers": {
      "root": {
        "level": "debug",
        "appender-ref": {
          "ref": "STDOUT",
          "ref": "List2"
        }
      },
      "logger": {
        "name": "org.openrepose.core.services.logging.LoggingServiceImplTest",
        "level": "debug"
      }
    }
  }
}
